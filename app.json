      "assetBundlePatterns": ["**/*"],
      "android": {
        "package": "com.fiap.bloggingapp",
        "expo": {
          "extra": {
            "API_URL": "http://192.168.15.24:3000"
          }
        },
          "plugins": [
            "expo-secure-store"
          ]
      }
    }
  }
  